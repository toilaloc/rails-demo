<% if session[:user_id] %>
Hello, <%= session[:fullname] %>
<%= link_to "Logout", logout_path, method: :delete %>
<% else %>
You're not login!!
<% end %>

<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Fullname</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.fullname %></td>
      </tr>
    <% end %>
  </tbody>
</table>